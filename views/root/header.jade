
style.
	.stead{
		fill: #60ba80;
		stroke:#01a38e;
		stroke-width:5;
		stroke-linecap:butt;
		stroke-linejoin:miter;
		stroke-miterlimit:4;
		stroke-opacity: 1;
		stroke-dasharray:none;
		display:inline;
		cursor: pointer;
		-webkit-transition: all ease 0.27s;
		-moz-transition: all ease 0.27s;
		-ms-transition: all ease 0.27s;
		-o-transition: all ease 0.27s;
		transition: all ease 0.27s;
	}
	.stead:hover .labeltext{
		/*stroke:green;
		stroke-opacity:1;
		stroke-width:5;
		fill:yellow;
		*/
		font-weight: bold;
		stroke: red;
	}
	
	.stead.busy{
		fill: #12846e !important;
	}


	.selected, .selected:hover{
		/* stroke:red; 
		stroke-opacity:1;
		stroke-width:5; 
		fill: #009881;*/
		fill: #8fc479;
	}

	.labeltext{
		font-size:			140px;
		font-style:			normal;
		font-weight:		normal;
		line-height:		0%;
		letter-spacing:		0px;
		word-spacing:		0px;
		fill:				#000000;
		fill-opacity:		1;
		stroke:				none;
		font-family:		Sans;
		cursor:				pointer;
	}
	.labelspan{
		text-align:center;
		line-height:0%;
		text-anchor:middle;
		fill:#ff0000;
		fill-opacity:1;
	}

	.s1{
		font-size:24px;
		fill: #d5edea;
		stroke: #d5edea;
		stroke-opacity:1;
		stroke-width:1;
	}

	.busy .s1{
		fill: #000000;
		stroke: #000000;
	}

	.s2{
		font-size:20px;
		fill: #91da0a;
		stroke: #91da0a;
		stroke-opacity:1;
		stroke-width:1;
	}

	.busy .s2{
		fill: #000000;
		stroke: #000000;
	}

	.onselected tspan.s1,.onselected tspan.s2{
		fill: #ffffff;
	}

	.stepen{
		font-size:65%;
		baseline-shift:super;
	}

