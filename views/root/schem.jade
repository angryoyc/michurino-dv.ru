.mpad.p2(ng-class='{active: mode}', collapse)
	.row.percent100
		.col-lg-3
			a.btn(href='#/main')
				small.glyphicon.glyphicon-chevron-left
				span  На главную

			div.details.top30()
				.row
					.col-xs-12.text-center.top30
						img(src='/static/images/logo_transp.png')
				div#proppanel.collapse
					.row
						.col-xs-12.lbl № участка:
						.col-xs-12.val {{data.curr.pp}}
					.row
						.col-xs-12.lbl Ряд:
						.col-xs-12.val {{data.curr.groupnum}}
					.row
						.col-xs-12.lbl № в кадастре:
						.col-xs-12.val {{data.curr.cadastr}}
					.row
						.col-xs-12.lbl Площадь, м*м:
						.col-xs-12.val {{data.curr.s}}
					.row
						.col-xs-12.lbl Стоимость:
						.col-xs-12.val
							strong {{data.cost * data.curr.s | currency}}
					.row
						.col-xs-12.text-center
							button.btn.top30(ng-click='stakeThis(data.curr)') забронировать


		.col-lg-9
			include mainsvg
